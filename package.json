{
  "name": "rowinbot.com",
  "version": "1.0.0",
  "description": "Rowin Hernandez personal website",
  "license": "GPL-3.0-only",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev:server": "node ./server.js",
    "content:watch": "node --watch --import ./content/builder/ts-node.register.mjs ./content/builder/content-builder.ts",
    "dev": "concurrently --kill-others-on-fail -n \"api,content\" -c \"auto\" \"npm run dev:server\" \"npm run content:watch\"",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@floating-ui/react": "^0.23.1",
    "@iconify-icon/react": "^2.1.0",
    "@mdx-js/rollup": "^3.0.1",
    "@react-spring/web": "^9.7.4",
    "@remix-run/express": "^2.11.0",
    "@remix-run/node": "^2.11.0",
    "@remix-run/react": "^2.11.0",
    "@resvg/resvg-js": "^2.4.1",
    "@rowinbot/mdx-code-formatter": "^1.1.10",
    "@rowinbot/rollup-image-blur": "^1.2.0",
    "clsx": "^1.2.1",
    "compression": "^1.7.4",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "esbuild": "^0.17.10",
    "express": "^4.19.2",
    "framer-motion": "^10.12.4",
    "isbot": "^4.1.0",
    "jotai": "^2.0.3",
    "mdx-bundler": "^9.2.1",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-slug": "^5.1.0",
    "satori": "^0.4.8",
    "sharp": "^0.32.0",
    "spin-delay": "^1.2.0",
    "tailwind-merge": "^1.12.0",
    "tiny-invariant": "^1.3.1",
    "use-sound": "^4.0.3",
    "vite": "^5.1.0",
    "watcher": "^2.3.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.11.0",
    "@remix-run/eslint-config": "^1.15.0",
    "@types/express": "^4.17.20",
    "@types/morgan": "^1.9.9",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.19",
    "chokidar": "^3.5.3",
    "eslint": "^8.27.0",
    "nodemon": "^2.0.21",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "prettier": "^2.8.7",
    "tailwindcss": "^3.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
